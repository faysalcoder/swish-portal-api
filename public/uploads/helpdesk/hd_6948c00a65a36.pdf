    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>

    <script>
      document.addEventListener("DOMContentLoaded", function () {
        // Set current year in footer
        document.getElementById("current-year").textContent =
          new Date().getFullYear();

        // Toggle sections functionality
        const toggleButton = document.getElementById("toggle-info-btn");
        const toggleLottie = document.getElementById("toggleLottie");
        const hiddenSections = document.querySelectorAll(".hidden-section");
        const staggerElements = document.querySelectorAll(".stagger-show");
        let sectionsVisible = false;

        // Initialize Lottie animation
        if (toggleLottie) {
          // You can customize Lottie animation properties here
          toggleLottie.addEventListener('mouseenter', () => {
            toggleLottie.setSpeed(1.5);
          });
          
          toggleLottie.addEventListener('mouseleave', () => {
            toggleLottie.setSpeed(1);
          });
        }

        // Toggle button click handler
        toggleButton.addEventListener("click", function () {
          if (!sectionsVisible) {
            // Show all hidden sections
            hiddenSections.forEach((section, index) => {
              setTimeout(() => {
                section.classList.remove("hidden-section");
                section.classList.add("show-section");
                
                // Add staggered animation to child elements
                const staggerShow = section.querySelector(".stagger-show");
                if (staggerShow) {
                  setTimeout(() => {
                    staggerShow.classList.add("show");
                  }, 300);
                }
              }, index * 100); // Stagger the appearance
            });
            
            // Change button text
            toggleButton.innerHTML = "Hide More Info";
            sectionsVisible = true;
            
            // Scroll to first shown section
            setTimeout(() => {
              document.getElementById("comparison-details").scrollIntoView({
                behavior: 'smooth'
              });
            }, 500);
            
          } else {
            // Hide all sections
            hiddenSections.forEach((section) => {
              section.classList.remove("show-section");
              section.classList.add("hidden-section");
              
              // Remove staggered animation
              const staggerShow = section.querySelector(".stagger-show");
              if (staggerShow) {
                staggerShow.classList.remove("show");
              }
            });
            
            // Change button text
            toggleButton.innerHTML = "Get More Info";
            sectionsVisible = false;
            
            // Scroll back to toggle section
            setTimeout(() => {
              document.getElementById("toggle-section").scrollIntoView({
                behavior: 'smooth'
              });
            }, 300);
          }
        });

        // Multi-step form state
        const formState = {
          currentStep: 1,
          totalSteps: 3,
          formData: {
            fullName: "",
            email: "",
            number: "",
            location: "",
            size: "",
            stage: "",
            buyTimeline: "",
            budget: "",
            requirements: "",
          },
        };

        // Multi-step modal functionality
        const multiStepModal = document.getElementById("multiStepModal");
        const openMultiStepModalButtons = document.querySelectorAll(
          ".open-multi-step-modal"
        );
        const closeMultiStepModalButton = document.querySelector(
          ".multi-step-modal-close"
        );
        const toast = document.getElementById("successToast");
        const toastClose = document.querySelector(".toast-close");
        let toastTimeout = null;

        // Open multi-step modal
        openMultiStepModalButtons.forEach((button) => {
          button.addEventListener("click", () => {
            multiStepModal.classList.add("active");
            document.body.style.overflow = "hidden";
            resetForm();
          });
        });

        // Close multi-step modal
        closeMultiStepModalButton.addEventListener("click", () => {
          multiStepModal.classList.remove("active");
          document.body.style.overflow = "auto";
        });

        // Close modal when clicking outside
        multiStepModal.addEventListener("click", (e) => {
          if (e.target === multiStepModal) {
            multiStepModal.classList.remove("active");
            document.body.style.overflow = "auto";
          }
        });

        // Toast close
        toastClose.addEventListener("click", () => {
          toast.classList.remove("show");
          if (toastTimeout) {
            clearTimeout(toastTimeout);
            toastTimeout = null;
          }
        });

        // Show toast
        function showToast(message) {
          if (toastTimeout) {
            clearTimeout(toastTimeout);
          }

          if (message) {
            const toastContent = toast.querySelector(".toast-content");
            toastContent.innerHTML = `
              <strong>Success!</strong>
              <p style="margin: 4px 0 0; font-size: 0.9rem">${message}</p>
            `;
          }

          toast.classList.add("show");
          toastTimeout = setTimeout(() => {
            toast.classList.remove("show");
          }, 10000);
        }

        // Form validation functions
        function validateStep(step) {
          const errors = {};
          const form = document.getElementById("multiStepForm");

          if (step === 1) {
            const fullName = form.fullName.value.trim();
            const email = form.email.value.trim();
            const number = form.number.value.trim();

            if (!fullName) errors.fullName = "Full Name is required";
            if (!email) {
              errors.email = "Email is required";
            } else if (!/^\S+@\S+\.\S+$/.test(email)) {
              errors.email = "Enter a valid email";
            }
            if (!number) {
              errors.number = "Phone number is required";
            } else if (!/^\d{11}$/.test(number.replace(/\D/g, ""))) {
              errors.number = "Phone must be 11 digits (Bangladesh)";
            }

            formState.formData.fullName = fullName;
            formState.formData.email = email;
            formState.formData.number = number;
          }

          if (step === 2) {
            const location = form.location.value.trim();
            const size = form.size.value;
            const stage = form.stage.value;

            if (!location) errors.location = "Location is required";
            if (!size) errors.size = "Select a size";
            if (!stage) errors.stage = "Select construction stage";

            formState.formData.location = location;
            formState.formData.size = size;
            formState.formData.stage = stage;
          }

          if (step === 3) {
            const buyTimeline = form.buyTimeline.value;
            const budget = form.budget.value;
            const requirements = form.requirements.value;

            if (!buyTimeline) errors.buyTimeline = "Select timeline";
            if (!budget) errors.budget = "Select budget";
            if (!requirements) errors.requirements = "Select a requirement";

            formState.formData.buyTimeline = buyTimeline;
            formState.formData.budget = budget;
            formState.formData.requirements = requirements;
          }

          // Clear all error states
          document.querySelectorAll(".is-invalid").forEach((el) => {
            el.classList.remove("is-invalid");
          });
          document.querySelectorAll(".invalid-feedback").forEach((el) => {
            el.style.display = "none";
          });

          // Show errors if any
          Object.keys(errors).forEach((field) => {
            const input = form[field];
            const feedback = input
              .closest(".form-group")
              .querySelector(".invalid-feedback");
            if (input && feedback) {
              input.classList.add("is-invalid");
              feedback.textContent = errors[field];
              feedback.style.display = "block";
            }
          });

          return Object.keys(errors).length === 0;
        }

        // Update progress bar
        function updateProgressBar() {
          document
            .querySelectorAll(".step-progress-bar")
            .forEach((bar, index) => {
              bar.classList.remove("active", "completed");
              if (index + 1 < formState.currentStep) {
                bar.classList.add("completed");
              } else if (index + 1 === formState.currentStep) {
                bar.classList.add("active");
              }
            });
        }

        // Switch steps
        function goToStep(step) {
          if (step < 1 || step > formState.totalSteps) return;

          // Hide all steps
          document.querySelectorAll(".form-step").forEach((stepEl) => {
            stepEl.classList.remove("active");
          });

          // Show current step
          document
            .querySelector(`.form-step[data-step="${step}"]`)
            .classList.add("active");

          // Update progress bar
          formState.currentStep = step;
          updateProgressBar();

          // Update button states
          document.querySelectorAll(".prev-step").forEach((btn) => {
            btn.disabled = step === 1;
          });

          // Scroll to top of modal content
          const modalBody = document.querySelector(".multi-step-modal-body");
          modalBody.scrollTop = 0;
        }

        // Next step handler
        document.querySelectorAll(".next-step").forEach((button) => {
          button.addEventListener("click", () => {
            if (validateStep(formState.currentStep)) {
              goToStep(formState.currentStep + 1);
            }
          });
        });

        // Previous step handler
        document.querySelectorAll(".prev-step").forEach((button) => {
          button.addEventListener("click", () => {
            goToStep(formState.currentStep - 1);
          });
        });

        // Form submission
        document
          .getElementById("multiStepForm")
          .addEventListener("submit", async (e) => {
            e.preventDefault();

            if (!validateStep(3)) {
              return;
            }

            const submitBtn = e.submitter;
            const originalText = submitBtn.innerHTML;

            // Show loading state
            submitBtn.innerHTML =
              '<i class="fas fa-spinner fa-spin"></i> Processing...';
            submitBtn.disabled = true;

            try {
              // Simulate API call (replace with actual API endpoint)
              const response = await fetch(
                "https://staging.swish.global/api/save_form.php",
                {
                  method: "POST",
                  headers: { "Content-Type": "application/json" },
                  body: JSON.stringify(formState.formData),
                }
              );

              if (response.ok) {
                // Success
                showToast(
                  "Please check your email inbox for your free drainage solution guide & SWISH product catalogue!"
                );

                // Close modal
                multiStepModal.classList.remove("active");
                document.body.style.overflow = "auto";

                // Simulate download
                setTimeout(() => {
                  const link = document.createElement("a");
                  link.href = "#";
                  link.download = "SWISH-Drainage-Solution-Guide.pdf";
                  document.body.appendChild(link);
                  link.click();
                  document.body.removeChild(link);
                }, 500);

                // Reset form
                resetForm();
              } else {
                throw new Error("Submission failed");
              }
            } catch (error) {
              console.error("Form submit error:", error);
              alert("Something went wrong. Please try again.");
            } finally {
              // Reset button
              submitBtn.innerHTML = originalText;
              submitBtn.disabled = false;
            }
          });

        // Reset form
        function resetForm() {
          document.getElementById("multiStepForm").reset();
          formState.currentStep = 1;
          formState.formData = {
            fullName: "",
            email: "",
            number: "",
            location: "",
            size: "",
            stage: "",
            buyTimeline: "",
            budget: "",
            requirements: "",
          };
          goToStep(1);
        }

        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
          anchor.addEventListener("click", function (e) {
            e.preventDefault();

            const targetId = this.getAttribute("href");
            if (targetId === "#") return;

            const targetElement = document.querySelector(targetId);
            if (targetElement) {
              window.scrollTo({
                top: targetElement.offsetTop - 80,
                behavior: "smooth",
              });

              // Update active nav link
              document.querySelectorAll(".nav-link").forEach((link) => {
                link.classList.remove("active");
              });
              this.classList.add("active");
            }
          });
        });

        // Navbar background on scroll
        window.addEventListener("scroll", function () {
          const navbar = document.querySelector(".navbar");
          if (window.scrollY > 50) {
            navbar.style.backgroundColor = "rgba(255, 255, 255, 0.98)";
            navbar.style.boxShadow = "0 5px 20px rgba(0, 0, 0, 0.1)";
          } else {
            navbar.style.backgroundColor = "rgba(255, 255, 255, 0.98)";
            navbar.style.boxShadow = "0 2px 20px rgba(0, 0, 0, 0.08)";
          }
        });

        // Scroll progress
        const scrollProgress = document.querySelector(".scroll-progress");
        window.addEventListener("scroll", () => {
          const windowHeight =
            document.documentElement.scrollHeight -
            document.documentElement.clientHeight;
          const scrolled = (window.scrollY / windowHeight) * 100;
          scrollProgress.style.width = scrolled + "%";
        });

        // Scroll animations
        const fadeElements = document.querySelectorAll(".fade-in");
        const observer = new IntersectionObserver(
          (entries) => {
            entries.forEach((entry) => {
              if (entry.isIntersecting) {
                entry.target.classList.add("visible");
              }
            });
          },
          {
            threshold: 0.1,
            rootMargin: "0px 0px -50px 0px",
          }
        );

        fadeElements.forEach((el) => observer.observe(el));

        // Highlight active problem card on scroll
        const problemCards = document.querySelectorAll(
          ".problem-solution-card"
        );
        const stickyCta = document.querySelector(".sticky-cta-card");

        const observerOptions = {
          root: null,
          rootMargin: "100px 0px -100px 0px",
          threshold: 0.3,
        };

        const problemObserver = new IntersectionObserver((entries) => {
          entries.forEach((entry) => {
            if (entry.isIntersecting) {
              entry.target.classList.add("active-problem");
            } else {
              entry.target.classList.remove("active-problem");
            }
          });
        }, observerOptions);

        problemCards.forEach((card) => {
          problemObserver.observe(card);
        });

        // Update CTA text based on visible problem
        const problemTextObserver = new IntersectionObserver(
          (entries) => {
            entries.forEach((entry) => {
              if (entry.isIntersecting) {
                const problemTitle =
                  entry.target.querySelector("h4").textContent;
                const ctaText = document.querySelector(
                  ".sticky-cta-card p.fw-bold"
                );
                if (ctaText && problemTitle.includes("Blockages")) {
                  ctaText.textContent = "Ready to end blockages forever?";
                } else if (ctaText && problemTitle.includes("Odor")) {
                  ctaText.textContent = "Ready to eliminate odors forever?";
                } else if (ctaText && problemTitle.includes("Insect")) {
                  ctaText.textContent = "Ready to stop insect intrusion?";
                } else if (ctaText && problemTitle.includes("Rust")) {
                  ctaText.textContent = "Ready to prevent rust forever?";
                }
              }
            });
          },
          { threshold: 0.5 }
        );

        problemCards.forEach((card) => {
          problemTextObserver.observe(card);
        });
      });
    </script>
  </body>
</html>